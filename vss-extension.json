{
    "manifestVersion": 1,
    "id": "ado-auth-tools",
    "name": "ADO Authentication Tools",
    "version": "0.2.4",
    "publisher": "MSFT-MORSE",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Tools for Rust support in ADO",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": "Tasks/AADAccessTokenAuth"
        },
        {
            "path": "Tasks/CargoAuthNew"
        },
        {
            "path": "Tasks/NuGetAuthenticateNew"
        }
    ],
    "contributions": [
        {
            "id": "aad-accesstoken-auth-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/AADAccessTokenAuth"
            }
        },
        {
            "id": "cargo-accesstoken-auth-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/CargoAuthNew"
            }
        },
        {
            "id": "nuget-accesstoken-auth-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/NuGetAuthenticateNew"
            }
        }
    ]
}
